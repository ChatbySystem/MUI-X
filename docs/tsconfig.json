{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    /* files are emitted by babel */
    "noEmit": true,
    "types": ["react", "node", "requestidlecallback"],
    "jsx": "preserve",
    "skipLibCheck": true,
    "incremental": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["src/pages/**/*", "data/**/*"],
  "exclude": ["docs/.next", "docs/export"]
}
