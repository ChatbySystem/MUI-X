<DataGrid
  columns={demoData.columns}
  pagination
  rowCount={rowCountState}
  {...rowsState}
  rows={data}
  loading={isLoading}
  paginationMode="server"
  onPageChange={(page) => setRowsState((prev) => ({ ...prev, page }))}
  onPageSizeChange={(pageSize) =>
    setRowsState((prev) => ({ ...prev, pageSize }))
  }
/>