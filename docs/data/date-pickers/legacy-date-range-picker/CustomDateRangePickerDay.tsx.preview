<StaticDateRangePicker
  displayStaticWrapperAs="desktop"
  label="date range"
  value={value}
  onChange={(newValue) => setValue(newValue)}
  slots={{ day: DateRangePickerDay }}
  renderInput={(startProps, endProps) => (
    <React.Fragment>
      <TextField {...startProps} />
      <Box sx={{ mx: 2 }}> to </Box>
      <TextField {...endProps} />
    </React.Fragment>
  )}
/>