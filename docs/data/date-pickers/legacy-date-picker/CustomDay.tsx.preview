<StaticDatePicker
  displayStaticWrapperAs="desktop"
  label="Week picker"
  value={value}
  onChange={(newValue) => {
    setValue(newValue);
  }}
  slots={{ day: Day }}
  slotProps={{
    day: {
      selectedDay: value,
    } as any,
  }}
  renderInput={(params) => <TextField {...params} />}
  inputFormat="'Week of' MMM d"
/>