{
  "name": "@material-ui/data-grid",
  "version": "4.0.0-alpha.2",
  "description": "The Material-UI community edition of the data grid component.",
  "author": "Material-UI Team",
  "main": "dist/index-cjs.js",
  "module": "dist/index-esm.js",
  "types": "dist/data-grid.d.ts",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "react-component",
    "material-ui",
    "react-table",
    "table",
    "datatable",
    "data-table",
    "datagrid",
    "data-grid"
  ],
  "scripts": {
    "precommit": "npm run lint",
    "prebuild": "rimraf build",
    "build": "cp -r ./src ./build && mkdir ./build/x-grid-modules && cp -r ../x-grid-modules/src ./build/x-grid-modules/src && cp -r ../x-grid-modules/lib/ ./build/x-grid-modules/lib && node replaces.js && rollup -c",
    "start": "rollup -cw",
    "lint": "eslint 'src/**/*.{ts,tsx}' --quiet --fix -c ../../../.eslintrc.js && npm run lint:css",
    "lint:css": "stylelint 'src/**/*.{ts,tsx}' ../../../.stylelintrc.js",
    "test": "../../../node_modules/.bin/jest --config jest.config.js",
    "typescript": "tsc -p tsconfig.json"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@material-ui/x-grid-modules": "^4.0.0-alpha.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.9.12",
    "react": "^16.8.0"
  },
  "setupFiles": [
    "<rootDir>/src/setupTests.js"
  ],
  "gitHead": "f1d56176bb6308bb55c69f5c2fd5593c4e4faebc",
  "engines": {
    "node": ">=8.0.0"
  }
}
